<!DOCTYPE html>
<html lang="pt-br">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Web Chat</title>
</head>
<body>
  <h1 class="title">CHAT MESSENGER</h1>
    <section class="nick">
      <span id="user">Bem-vindo! Seu nickname Ã© </span>
    </section>
    <section class="allContent">
      <ul id="msgContainer" class="msgContainer">
        <% renderMessages.forEach((msg) => { %>
          <li><%= msg %></li>
        <% }) %>
      </ul>
    </section>
    <section class="msgForm">
      <input type="text"id="msgInput">
      <button type="submit"id="msgBtn">Enviar</button>
    </section>
    <script src="/socket.io/socket.io.js"></script>
    <script src="../js/webChat.js"></script>
</body>
</html>