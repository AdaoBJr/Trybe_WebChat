<!DOCTYPE html>
<html>

  <head>
    <title>Socket.IO chat</title>
    <!-- <link rel="stylesheet" type="text/css" href="./css/index.css"> -->
  </head>

  <body>

    <form id="new-user" action="">
      <input data-testid="nickname-box" id="input-nick"/>
      <button data-testid="nickname-button" id="button-nick">Send New Nick</button>
    </form>

    <p id="user" data-testid="online-user"></p>
    <ul id="messages"></ul>

    <form id="form" action="">
      <input id="input" autocomplete="off" data-testid="message-box"/>
      <button data-testid="send-button">
        Send
      </button>
    </form>

  </body>
  <script src="/socket.io/socket.io.js"></script>
  <script src="/js/message.js"></script>

</html>
