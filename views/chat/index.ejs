<!DOCTYPE html>
<html>
  <head>
    <title>Socket.IO - trybe</title>
    <link rel="stylesheet" href="./css/chat.css">
  </head>
  <body>
    <ul data-testid="message-box" id="messages"></ul>
    <form id="formsMessage" action="">
      <input id="messageInput" autocomplete="off" />
      <p id="nickName" data-testid="online-user"></p>
      <button data-testid="send-button">Send</button>
    </form>
    <form id="formsNickName" action="">
      <input id="nickInput" data-testid="nickname-box" autocomplete="off" />
      <button data-testid="nickname-button">Change</button>
    </form>
    <script src="/socket.io/socket.io.js"></script>
    <script src='./js/chat.js'></script>
  </body>
</html>